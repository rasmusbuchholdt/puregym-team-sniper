{
  "name": "puregym-team-sniper",
  "version": "1.0.0",
  "description": "PureGym Team Sniper",
  "main": "node dist/cli.js",
  "scripts": {
    "develop": "tsc -w",
    "start": "node dist/cli.js",
    "cli": "node dist/cli.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --format stylish --max-warnings 0 .",
    "lint:prettier": "prettier --list-different .",
    "eslint:fix": "eslint . --fix",
    "postinstall": "tsc",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rasmusbuchholdt/puregym-team-sniper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rasmusbuchholdt/puregym-team-sniper/issues"
  },
  "homepage": "https://github.com/rasmusbuchholdt/puregym-team-sniper#readme",
  "dependencies": {
    "axios": "^1.6.8",
    "commander": "^10.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/qs": "^6.9.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
